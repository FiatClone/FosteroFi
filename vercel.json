{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "regions": ["iad1"],
  "functions": {
    "app/**/*.ts": {
      "maxDuration": 30
    },
    "app/**/*.tsx": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "env": {
    "NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID": "c5df0951573e834c17ba8d61110b38bd",
    "NEXT_PUBLIC_ALCHEMY_API_KEY": "0Zoy-lsVH2iXqNBlZzaYPT1I0PlCf701",
    "NEXT_PUBLIC_INFURA_API_KEY": "9a0c1339645f42af855bf3e9dbe56a7a"
  }
}